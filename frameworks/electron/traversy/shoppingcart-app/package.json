  "bugs": {
    "url": "https://github.com/ArthurGartner/electron-tutorial-traversy/issues"
  },
  "homepage": "https://github.com/ArthurGartner/electron-tutorial-traversy#readme",
  "dependencies": {
    "electron": "^18.2.0"
  },
  "devDependencies": {
    "electron-packager": "^15.5.1"
  }
}
